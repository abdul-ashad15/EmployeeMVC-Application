<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Get Employees By ID</title>
</head>
<body>
    <div>
        <table style="font-family:Arial">
            <tr>
                <td>
                    @Html.Label("Employee ID:")
                </td>
                <td>
                    @Model.EmpId
                </td>
            </tr>
            <tr>
                <td>
                    @Html.Label("First Name:")
                </td>
                <td>
                    @Model.FirstName
                </td>
            </tr>
            <tr>
                <td>
                    @Html.Label("Last Name:")
                </td>
                <td>
                    @Model.LastName
                </td>
            </tr>
        </table>
    </div>
</body>
</html>
